{
  "name": "@wearesage/vue",
  "version": "2.3.0",
  "type": "module",
  "main": "./src/client.ts",
  "types": "./src/client.ts",
  "exports": {
    ".": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    },
    "./vite-preset": {
      "types": "./dist/vite-preset.d.ts",
      "default": "./dist/vite-preset.js"
    },
    "./assets/*": "./src/assets/*",
    "./classes/*": "./src/classes/*",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*",
    "./components/audius": "./src/components/audius/index.ts",
    "./components/chat": "./src/components/chat/index.ts",
    "./components/chess": "./src/components/chess/index.ts",
    "./components/common": "./src/components/common/index.ts",
    "./components/forms": "./src/components/forms/index.ts",
    "./components/home": "./src/components/home/index.ts",
    "./components/layout": "./src/components/layout/index.ts",
    "./components/webgl": "./src/components/webgl/index.ts",
    "./composables": "./src/composables/index.ts",
    "./composables/*": "./src/composables/*",
    "./constants/*": "./src/constants/*",
    "./css": "./src/styles/global/_index.scss",
    "./data/*": "./src/data/*",
    "./sass": "./src/styles/_index.scss",
    "./styles/*": "./src/styles/*",
    "./stores": "./src/stores/index.ts",
    "./stores/*": "./src/stores/*",
    "./router": "./src/router/index.ts",
    "./router/*": "./src/router/*",
    "./util": "./src/util/index.ts",
    "./util/*": "./src/util/*"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build:vite": "tsc --project tsconfig.vite.json",
    "prepublishOnly": "npm run build:vite"
  },
  "author": "contact@zachwinter.com",
  "license": "MIT",
