  "shader": "#define orbs 20.\n\n  void main () {\n    vec2 uv = -1. + 2.* gl_FragCoord.xy / resolution.xy.xy;\n    uv.x *= resolution.x/resolution.y;\n    uv *= zoom;\n    uv *= k_rotate2d(stream/62.);\n    float dist = length(sin(cos(uv+stream) + stream/3.5));\n    for (float i = 0.; i < orbs; i++) {\n      uv.x *= abs(dist+.0001* dist*(i) * (uv.x));\n      float t = (i+1.) * PI / orbs * 2.;\n      float x = radius * tan(t+stream/-4.);//* cos(t-stream/1.) / sin(t);\n      float y = radius * sin(t-stream/2.);// / sin(t-stream/1.) / cos(t);\n      vec2 position = vec2(x, y);\n      vec3 color = cos(vec3(-2, 0, -1) * PI * 2. / 3. + PI * (float(i+100.) / colorShift)) * 0.5 + 0.5;\n      gl_FragColor += k_orb(uv, orbSize, position, color, contrast);\n    }\n  }",
